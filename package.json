{
  "name": "eslint-config-qcolate",
  "author": "qcolate@outlook.com",
  "private": false,
  "sideEffect": false,
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6.4.0",
    "yarn": ">=1.15"
  },
  "main": "index.js",
  "homepage": "https://github.com/qcolate/eslint-config-qcolate",
  "bugs": {
    "url": "https://github.com/qcolate/eslint-config-qcolate/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean && rimraf yarn.lock && rimraf node_modules",
    "fix": "lerna run fix",
    "tsc-check": "yarn workspaces run tsc-check",
    "---------------------------------------------------------------------- LINE ----": "----",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "lerna publish from-package --dist-tag latest"
  },
  "dependencies": {},
  "devDependencies": {
    "commitlint": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/config-lerna-scopes": "^9.1.2",
    "eslint": "7.4.0",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8"
  }
}
