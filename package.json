{
  "name": "eslint-config-qcolate",
  "author": "solidzoro <solidzoro@live.com>",
  "version": "1.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6.4.0",
    "yarn": ">=1.15"
  },
  "main": "index.js",
  "homepage": "https://github.com/qcolate/eslint-config-qcolate",
  "repository": {
    "type": "git",
    "url": "https://github.com/qcolate/eslint-config-qcolate.git"
  },
  "bugs": {
    "url": "https://github.com/qcolate/eslint-config-qcolate/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0 -s",
    "npm-publish": "git push --follow-tags origin master && npm publish",
    "versionup": "standard-version",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "release": "yarn versionup && yarn npm-publish",
    "release:minor": "yarn versionup:minor && yarn npm-publish",
    "release:major": "yarn versionup:major && yarn npm-publish"
  },
  "keywords": [
    "eslint",
    "airbnb",
    "config",
    "eslintconfig",
    "javascript",
    "styleguide",
    "typescript"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "commitlint": "^9.1.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "typescript": "^4.0.2"
  }
}
